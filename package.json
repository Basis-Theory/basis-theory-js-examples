{
  "name": "basis-theory-js-examples",
  "version": "1.0.0",
  "description": "Basis Theory Vanilla JS examples",
  "main": "index.js",
  "repository": "https://github.com/Basis-Theory/basis-theory-js-examples",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": ["collect-atomic-cards-with-elements", "collect-atomic-banks-with-elements", "store-atomic-banks"],
  "scripts": {
    "lint": "eslint . --quiet --ignore-path .gitignore",
    "start:collect-atomic-cards-with-elements": "yarn workspace collect-atomic-cards-with-elements start",
    "start:collect-atomic-banks-with-elements": "yarn workspace collect-atomic-banks-with-elements start",
    "start:store-atomic-banks": "yarn workspace store-atomic-banks start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless -b chrome",
    "cypress:run:collect-atomic-cards-with-elements": "yarn cypress:run --spec \"cypress/integration/collect-atomic-cards-with-elements/**/*\"",
    "cypress:run:collect-atomic-banks-with-elements": "yarn cypress:run --spec \"cypress/integration/collect-atomic-banks-with-elements/**/*\"",
    "cypress:run:store-atomic-banks": "yarn cypress:run --spec \"cypress/integration/store-atomic-banks/**/*\""
  },
  "devDependencies": {
    "@basis-theory/eslint-config": "^1.0.6",
    "@types/chance": "^1.1.3",
    "chance": "^1.1.8",
    "cypress": "^9.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "^2.5.1"
  },
  "prettier": "@basis-theory/eslint-config/prettier"
}
